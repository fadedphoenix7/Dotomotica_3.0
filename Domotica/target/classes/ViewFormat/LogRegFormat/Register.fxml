<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.text.*?>
<?import com.jfoenix.controls.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>

<Pane id="register" fx:id="Pane" maxHeight="800.0" maxWidth="600.0" minHeight="800.0" minWidth="600.0" prefHeight="600.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.jose.controller.LogReg.LogRegController">
   <children>
      <Pane layoutX="150.0" layoutY="100.0" prefHeight="400.0" prefWidth="500.0" style="-fx-background-color: #ffffff;">
         <children>
            <JFXTextField fx:id="emailText" labelFloat="true" layoutX="150.0" layoutY="80.0" maxWidth="200.0" prefHeight="30.0" prefWidth="200.0" promptText="Email">
               <font>
                  <Font name="Gadugi" size="16.0" />
               </font>
            </JFXTextField>
            <JFXTextField fx:id="nameText" labelFloat="true" layoutX="150.0" layoutY="140.0" maxWidth="200.0" prefHeight="30.0" prefWidth="200.0" promptText="Name">
               <font>
                  <Font name="Gadugi" size="16.0" />
               </font>
            </JFXTextField>
            <JFXTextField fx:id="lastNameText" labelFloat="true" layoutX="150.0" layoutY="200.0" maxWidth="200.0" prefHeight="30.0" prefWidth="200.0" promptText="Last name">
               <font>
                  <Font name="Gadugi" size="16.0" />
               </font>
            </JFXTextField>
            <JFXButton fx:id="verifyEmailButton" layoutX="374.0" layoutY="80.0" onAction="#verifyIsEmailRegister" text="Verify">
               <font>
                  <Font name="Gadugi" size="16.0" />
               </font>
            </JFXButton>
            <JFXButton layoutX="210.0" layoutY="335.0" onAction="#registerNewUser" text="Register">
               <font>
                  <Font name="Gadugi" size="16.0" />
               </font>
            </JFXButton>
            <JFXPasswordField fx:id="passwordText" labelFloat="true" layoutX="150.0" layoutY="260.0" prefHeight="30.0" prefWidth="200.0" promptText="Password">
               <font>
                  <Font name="Gadugi" size="16.0" />
               </font>
            </JFXPasswordField>
            <JFXButton layoutX="14.0" layoutY="14.0" onAction="#backToLogin" text="Back">
               <font>
                  <Font name="Gadugi" size="16.0" />
               </font>
            </JFXButton>
            <Label fx:id="houseName" layoutX="150" layoutY="10" text="#aaaa " textAlignment="CENTER">
               <font>
                  <Font name="Gadugi" size="25.0" />
               </font>
            </Label>
            <Label fx:id="emailLabel" layoutX="150" layoutY="120" text=" " textAlignment="CENTER">
               <font>
                  <Font name="Gadugi" size="16" />
               </font>
            </Label>
            <Label fx:id="houseName1" layoutX="150.0" layoutY="297.0" prefHeight="25.0" prefWidth="189.0" text="minimun size: 8" textAlignment="CENTER">
               <font>
                  <Font name="Gadugi" size="14.0" />
               </font>
            </Label>
         </children>
      </Pane>
      <JFXButton id="CloseButton" fx:id="closeButton" layoutX="762.0" layoutY="15.0" onAction="#closeWindow" text="X">
         <font>
            <Font name="Gadugi" size="12.0" />
         </font>
      </JFXButton>
   </children>
</Pane>
